# Environment Variables
# Copy this file to .env for local development

# Base path for the application (used for GitHub Pages deployment)
# Default: /sdge-rate-explorer/
# For local dev or custom domain: /
VITE_BASE_PATH=/sdge-rate-explorer/

# Data URL for the rates JSON file
# Default: /rates.json (relative to base path)
VITE_DATA_URL=/rates.json

# Enable development mode features (debug logs, etc.)
# Default: false
VITE_DEV_MODE=false
